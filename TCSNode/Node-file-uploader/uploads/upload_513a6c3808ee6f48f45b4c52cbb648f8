        <script>
            function login(){
                var uname=document.getElementById("username").value;
                var pwd = document.getElementById("password").value;
                event.preventDefault();
	            var form = event.target.form;
                if(uname==''|| pwd==''){
                 swal('Oops!!','Username/Password cannot be empty','error').then
                 (function(){
                     location="login.html";
                 })
                }
            }
        </script>

